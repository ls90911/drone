function [] = plot_sensor()
%UNTITLED 此处显示有关此函数的摘要
%   此处显示详细说明
global sensor_states sensor_states_raw drone_states time pointer
figure(4)
%subplot(1,3,1)
plot(time(1:pointer-1),drone_states(10,1:pointer-1),'r',time(1:pointer-1),sensor_states_raw(10,1:pointer-1),'b',...
    time(1:pointer-1),sensor_states(10,1:pointer-1),'');
xlabel('t[s]');
ylabel('p[rad/s^2]');
legend('real states','measured stetes','filtered states');



end

